line_read = input()       
counter_plus = 0        
counter_star = 0      
plus = '+'
star = '*'
for i in range(len(line_read)):      
    if line_read[i] in plus:           
        counter_plus += 1          
    if line_read[i] in star:            
        counter_star += 1
print('Символ', '+', 'встречается', counter_plus, 'раз')   
print('Символ', '*', 'встречается', counter_star, 'раз')  
