s = input()      # считал строку 
count = 0        # cчетчик для '+'
counter = 0      # счетчик для '*'
a = '+'
b = '*'
for i in range(len(s)):      # цикл разбивает строку на символы 
    if s[i] in a:            # за каждый плюс счетчик увеличивается на 1
        count += 1           
    if s[i] in b:            # за каждое умножение счетчик увеличивается на 1
        counter += 1
print('Символ', '+', 'встречается', count, 'раз', sep = ' ')   # вывожу ответ
print('Символ', '*', 'встречается', counter, 'раз', sep = ' ')        
